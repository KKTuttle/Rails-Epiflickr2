<h1>EpiFlickr</h1>

<h3>Browse Images</h3>
<div class="image-list">
  <% @images.each do |image| %>
    <div class="image-div">
      <%= image_tag image.image.url(:medium), class: "img-responsive" %>
      <%= link_to image.title, user_image_path(image.user_id, image) %>
    </div>
  <% end %>
</div>
